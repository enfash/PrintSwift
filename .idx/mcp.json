{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-filesystem",
        "--root",
        "."
      ]
    },
    "exec": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-exec"
      ]
    },
    "git": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-git"
      ]
    },
    "browser": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-browser"
      ]
    },
    "servss": {
      "command": "npx",
      "args": [
        "-",
        "y",
        "myorg/server"
      ]
    }
  }
}